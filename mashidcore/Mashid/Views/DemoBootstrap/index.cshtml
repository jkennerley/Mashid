@model IEnumerable<Question>

<hr />
<div class="container">
    @foreach (var q in this.Model)
    {
        <div class="row">
            <div class="col">
                <div class="bg-primary text-white">
                    @q.Id
                </div>
            </div>
            <div class="col">
                <div class="bg-success text-white">
                    @q.Text
                </div>
            </div>
        </div>
    }
</div>


<hr />

<div class="container-fluid">
    @foreach (var q in this.Model)
    {
        <div class="row">
            <div class="col">
                <div class="bg-primary text-white">
                    @q.Id
                </div>
            </div>
            <div class="col">
                <div class="bg-success text-white">
                    @q.Text
                </div>
            </div>
        </div>
    }
</div>


<hr />






